{
    "name": "serafim/real-mvc",
    "description": "An example of real mvc project",
    "license": "WTFPL",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "react/http": "~0.8",
        "ramsey/uuid": "~3.9",
        "psr/log": "~1.1",
        "cboden/ratchet": "~0.4",
        "vlucas/phpdotenv": "~3.6",
        "doctrine/annotations": "~1.8",
        "railt/container": "1.4.x-dev@dev",
        "laminas/laminas-diactoros": "~2.2"
    },
    "require-dev": {
        "monolog/monolog": "~2.0",
        "symfony/var-dumper": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Kernel\\": "src/"
        }
